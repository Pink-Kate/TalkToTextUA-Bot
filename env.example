# Токен бота Telegram
# Отримайте його у @BotFather
BOT_TOKEN=your_bot_token_here

# Рівень логування (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Username або User ID адміністратора (для команди /stats)
